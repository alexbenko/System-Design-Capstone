{
  "name": "reviews_component",
  "version": "1.0.0",
  "description": "replica of lululemon reviews module for educational purposes",
  "main": "./server/index.js",
  "scripts": {
    "test": "jest",
    "start": "nodemon",
    "build": "webpack --w --mode development",
    "build-prod": "webpack --mode production",
    "csv": "node database/csv.js ",
    "count": "node database/mongo/counter.js",
    "drop": "node database/mongo/drop.js && npm run count",
    "psql": "psql -d sdc -U root",
    "schema": "psql -d sdc -U root -f schema.sql",
    "psql-seed": "psql -d sdc -U AlexB -c '\\COPY reviews(rev_id,rating,title,review,recommendation,nickname,email,age,bodyType,locat,wearTo,likes,dislikes) FROM 'sdc.csv' csv header ;'",
    "stress-get": "artillery run stress-test/get.yml",
    "stress-post": "artillery run stress-test/post.yml",
    "aws": "aws s3 sync client/dist s3://sdc-alex --acl public-read",
    "s3": "npm run build-prod && npm run aws"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JJACD/reviews_component.git"
  },
  "author": "Alex Benko, Danny San",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "artillery": "^1.6.0",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "csv-write-stream": "^2.0.0",
    "csvtojson": "^2.0.10",
    "express": "4.17.1",
    "faker": "^4.1.0",
    "moment": "^2.25.3",
    "morgan": "1.10.0",
    "newrelic": "^6.8.0",
    "nodemon": "2.0.2",
    "pg": "^8.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@shelf/jest-mongodb": "^1.1.5",
    "babel-jest": "^26.0.1",
    "babel-loader": "8.1.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.1.3",
    "supertest": "^4.0.2",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "bugs": {
    "url": "https://github.com/JJACD/reviews_component/issues"
  },
  "homepage": "https://github.com/JJACD/reviews_component#readme"
}
